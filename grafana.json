          "rawSql": "WITH RECURSIVE  dates(var1, var2) as(\r\n\tVALUES(TO_TIMESTAMP($__timeFrom(),'YYYY-MM-DDTHH24:MI:SS') + INTERVAL '1 hour', TO_TIMESTAMP($__timeTo(),'YYYY-MM-DDTHH24:MI:SS') + INTERVAL '2 hour')\r\n),\r\ntheDATES AS(\r\n\tSELECT dates.var1 as theDate\r\n\tFROM dates\r\n\tUNION ALL \r\n\tSELECT theDate + INTERVAL '1 Minute'\r\n\tFROM theDATES, dates\r\n\tWHERE theDate + INTERVAL '1 Minute' <= dates.var2\r\n),\r\ntheValue(ertek,dates) as(\r\nSELECT COUNT(*) as ertek, make_timestamp(extract(year from request_log_timestamp)::int,extract(month from request_log_timestamp)::int, extract(day from request_log_timestamp)::int, extract(hour from request_log_timestamp)::int, extract(minute from request_log_timestamp)::int,0::int) as timei\r\nFROM telegram_ui_table\r\nWHERE $__timeFilter(request_log_timestamp) and\r\ntelegram_name_var IN ($telegramnamevar)\r\nGROUP BY make_timestamp(extract(year from request_log_timestamp)::int,extract(month from request_log_timestamp)::int, extract(day from request_log_timestamp)::int, extract(hour from request_log_timestamp)::int, extract(minute from request_log_timestamp)::int,0)\r\n)\r\n\r\nSELECT theDate as time, COALESCE(theValue.ertek,0) as value\r\nFROM theDATES\r\nLEFT join theValue\r\non make_timestamp(extract(year from theDate)::int,extract(month from theDate)::int, extract(day from theDate)::int, extract(hour from theDate)::int, extract(minute from theDate)::int,0) = theValue.dates\r\nORDER BY theDate",
